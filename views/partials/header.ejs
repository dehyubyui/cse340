
<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (locals.responseLogin) { %>
      <% if (locals.accountType === 'Client') { %>
        <a title="Click to manage account" href="/account/management">Welcome <%= locals.firstName %></a>
      <% } else { %>
        <a title="Click to go dashboard" href="/account">Dashboard</a>
        <a title="Click to log out" href="/logout">Log Out</a>
      <% } %>
    <% } else { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } %>
  </div>
</header>